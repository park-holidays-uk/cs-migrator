{
  "name": "cs-migrator",
  "version": "1.0.0",
  "description": "Tool to migrate data into contentstack",
  "main": "index.js",
  "scripts": {
    "build": "npx webpack --mode=development",
    "migrateSlugs": "npm run build && node ./dist/migrateSlugs.js parkholidays",
    "migrateCmsTags": "npm run build && node ./dist/migrateCmsTags.js parkholidays",
    "migrateCompanyBrand": "npm run build && node ./dist/migrateCompanyBrand.js parkholidays",
    "migrateIsVisible": "npm run build && node ./dist/migrateIsVisible.js parkholidays",
    "migrateMediaTextContent": "npm run build && node ./dist/migrateMediaTextContent.js parkholidays",
    "publish": "npm run build && node ./dist/publish.js",
    "dynamo": "npm run build && node ./dist/dynamo.js",
    "types": "npm run build && node ./dist/contentTypes.js",
    "start": "npm run build && node ./dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.10",
    "aws-sdk": "^2.1055.0",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "form-data": "^4.0.0",
    "pluralize": "^8.0.0",
    "promise-mysql": "^5.0.3",
    "prompt-sync": "^4.2.0",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "happypack": "^5.0.1",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  }
